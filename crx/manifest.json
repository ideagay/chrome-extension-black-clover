{
    "name": "Black Clover",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "日常辅助插件",
    "devtools_page": "devtools.html",
    "browser_action": {
        "default_icon": "icons/icons.png",
        "default_popup": "popup.html"
    },
    "background":{
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs",
        "http://*/",
        "https://*/",
        "cookies",
        "unlimitedStorage",
        "notifications"
    ]
}